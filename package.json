{
	"name": "temporary",
	"version": "0.0.0",
	"scripts": {
		"start": "ng serve",
		"commit": "cz",
		"build": "ng build",
		"lint": "ng lint",
		"lint:fix": "ng lint --fix",
		"lint:style": "npx stylelint '**/*.sass'",
		"lint:style:fix": "npx stylelint '**/*.sass' --fix",
		"prettify": "npx prettier --write src/**/*.{ts,html}",
		"dev:ssr": "ng run temporary:serve-ssr",
		"serve:ssr": "node dist/temporary/server/main.js",
		"build:ssr": "ng build --prod && ng run temporary:server:production",
		"prerender": "ng run temporary:prerender",
		"deploy": "ng deploy",
		"deploy:preview": "ng deploy --preview"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~11.2.7",
		"@angular/common": "~11.2.7",
		"@angular/compiler": "~11.2.7",
		"@angular/core": "~11.2.7",
		"@angular/fire": "6.1.4",
		"@angular/forms": "~11.2.7",
		"@angular/platform-browser": "~11.2.7",
		"@angular/platform-browser-dynamic": "~11.2.7",
		"@angular/platform-server": "~11.2.7",
		"@angular/router": "~11.2.7",
		"@nguniversal/express-engine": "11.2.1",
		"express": "^4.15.2",
		"firebase": "^7.0 || ^8.0",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.3"
	},
	"devDependencies": {
		"@angular-devkit/architect": ">= 0.900 < 0.1200",
		"@angular-devkit/build-angular": "~0.1102.6",
		"@angular-eslint/builder": "2.0.2",
		"@angular-eslint/eslint-plugin": "2.0.2",
		"@angular-eslint/eslint-plugin-template": "2.0.2",
		"@angular-eslint/schematics": "2.0.2",
		"@angular-eslint/template-parser": "2.0.2",
		"@angular/cli": "~11.2.6",
		"@angular/compiler-cli": "~11.2.7",
		"@commitlint/cli": "^12.0.1",
		"@commitlint/config-angular": "^12.0.1",
		"@nguniversal/builders": "^11.2.1",
		"@types/express": "^4.17.0",
		"@types/jasmine": "~3.6.0",
		"@types/node": "^12.11.1",
		"@typescript-eslint/eslint-plugin": "4.16.1",
		"@typescript-eslint/parser": "4.16.1",
		"commitizen": "^4.2.3",
		"cz-customizable": "^6.3.0",
		"eslint": "^7.6.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-jsdoc": "30.7.6",
		"eslint-plugin-prefer-arrow": "1.2.2",
		"firebase-admin": "^8.10.0",
		"firebase-functions": "^3.6.0",
		"firebase-functions-test": "^0.2.2",
		"firebase-tools": "^8.0.0",
		"fuzzy": "^0.1.3",
		"husky": "~4.3.8",
		"inquirer": "^6.2.2",
		"inquirer-autocomplete-prompt": "^1.0.1",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.1.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.0.3",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"lint-staged": "^10.5.4",
		"open": "^7.0.3",
		"prettier": "^2.2.1",
		"protractor": "~7.0.0",
		"stylelint": "^13.12.0",
		"stylelint-config-sass-guidelines": "^8.0.0",
		"stylelint-prettier": "^1.2.0",
		"ts-node": "~8.3.0",
		"typescript": "~4.1.5"
	},
	"husky": {
		"hooks": {
			"pre-commit": "ng lint && lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"./src/**/*.{ts,html,sass}": [
			"npx prettier --write",
			"npx stylelint '**/*.sass' --fix",
			"eslint src/**/*.ts --fix"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	}
}
